# Environment Configuration Template
# Copy this file to .env and update with your actual Twilio credentials

# Twilio Configuration (Required)
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_API_KEY=your_twilio_api_key_here
TWILIO_API_SECRET=your_twilio_api_secret_here
TWILIO_TWIML_APP_SID=your_twiml_application_sid_here
TWILIO_PHONE_NUMBER=your_twilio_phone_number_here

# Application Configuration (Optional)
PORT=3000
NODE_ENV=development

